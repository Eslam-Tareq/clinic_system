{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "./",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.21",
      "maxDuration": 30
    }
  }
}
